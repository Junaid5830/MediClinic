
@{
    ViewData["Title"] = "I.M.E Edit";
    Layout = "~/Views/Shared/_PatientLayout.cshtml";
}
@section styles{ 
    <style>
        .input-field{
            margin:0 auto   
        }
    </style>

}
@section scripts{
    <script>
        $(document).ready(function () {

            $('.Place').autocomplete({
                data: {
                    "Bronx NY": null,
                    "New Jersy": null,
                },
                limit: 10,
                minLength: 0
            });
            $('.Representative').autocomplete({
                data: {
                    " Smart Guy, Esq": null,
                    "Rosy": null,
                },
                limit: 10,
                minLength: 0
            });
            $('.Status').autocomplete({
                data: {
                    " No Show": null,
                },
                limit: 10,
                minLength: 0
            });
            $('.Reason').autocomplete({
                data: {
                    " N/A": null,
                },
                limit: 10,
                minLength: 0
            });
            $('.Transcripts').autocomplete({
                data: {
                    "Yes": null,
                    "No": null,
                },
                limit: 10,
                minLength: 0
            });

        });
    </script>
}
<div id="breadcrumbs-wrapper">
    <div class="container">
        <div class="row">
            <div class="col s6 m6 ">
                <h5 class="breadcrumbs-title color-blue mt-0">I.M.E</h5>
            </div>

            <div class="col s6 m6 ">
                <a class="waves-effect waves-light float-right" style="margin-top: -1px;" href="/Home/PatientHippaRelease">
                    <i class="material-icons  right arrow-icon">arrow_back</i>

                </a>
            </div>
        </div>
    </div>
</div>
<div class="container">
    <div class="row">
        <form class="form-inline col s12">
            <div class="row">
                    <div class="col s12 m6 ">
                        <div class="input-field">
                            <p>Date of IME</p>
                            <input type="text" class="datepicker text-input" placeholder="Select Date">
                        </div>

                    </div>
                    <div class="col s12 m6 ">
                        <div class="input-field">
                            <p>Time of IME</p>
                            <input type="text" class="timepicker text-input" placeholder="Select Time">
                        </div>
                    </div>
             

            </div>
            <div class="row">
                <div class=" col s12 m6">
                    <div class="input-field">
                        <p>Place Of EUO</p>
                        <div class="dropdownauto"><input class="text-input input-styling Place" type="text" placeholder="Select Place"></div>
                    </div>
                </div>
                <div class=" col s12 m6">

                    <div class="input-field">
                        <p>Represented By</p>

                        <div class="dropdownauto"><input class="text-input input-styling Representative" type="text" placeholder="Select Representative"></div>
                    </div>

                </div>
            </div>
            <div class="row">
                    <div class="col s12 m6">
                        <div class="input-field">
                            <p>Status Of EUO</p>
                            <div class="dropdownauto"><input class="text-input input-styling Status" type="text" placeholder="Select Status"></div>
                        </div>
                    </div>
                    <div class=" col s12 m6">

                        <div class="input-field ">
                            <p>Reason</p>
                            <div class="dropdownauto"><input class="text-input input-styling Reason" type="text" placeholder="Select Reason"></div>
                        </div>
                    </div>
               
            </div>
            <div class="row">
                    <div class="col s12 m6">
                        <div class="input-field">
                            <p>Transcripts</p>
                            <div class="dropdownauto"><input class="text-input input-styling Transcripts" type="text" placeholder="Select Transcripts"></div>
                        </div>
                    </div>
                

            </div>
            <div class="row mt-2 mb-2">
                <div class="col s12 m12" style="text-align:center">
                    <a href="javascript:void(0);" class="btn add-btn">Save</a>
                    <a href="javascript:void(0);" class="Recreate btn">Edit</a>
                    <a href="@Url.Action("PatientHippaRelease", "Home")" class="Recreate btn">Cancel</a>
                </div>
            </div>
        </form>

    </div>
</div>

