
@{
    ViewData["Title"] = "DMESuppliesEditElite";
    Layout = "~/Views/Shared/_LayoutElite.cshtml";
}

@section styles{

    <style>


        .DMeSupplies p {
            margin-top: 1rem !important;
        }
    </style>
}
@section scripts{

    <script>
        $(function () {


            $('.selectpicker').selectpicker();
        });
        $(".select2").select2();

    </script>

    <script>
        $(document).ready(function () {
            $('.dropify').dropify();
        });
    </script>

}
<div class="page-wrapper">
    <div class="container">
        <div id="breadcrumbs-wrapper">
            <!-- Search for small screen-->
           
                <div class="row">
                    <div class=" col" margin-top-8">
                        <h5 class="breadcrumbs-title color-blue">DME Supplies/Equipment</h5>
                    </div>

                    <div class="col">
                        <a href="@Url.Action("DMEsuppliesElite","PatientSideElite")" class="waves-effect waves-light float-right" style="margin-top: -11px">
                            <i class="mdi mdi-arrow-left right arrow-icon"></i>

                        </a>
                    </div>

            </div>
        </div>
        <section class="DMeSupplies">
          
                <div class="row">
                    <form class="col-sm-12  ">
                        <div class="row">
                            <div class="col-md-6 ">
                                <p>
                                    Date of Prescription

                                </p>

                                <div class="input-field">
                                    <input type="date" class="text-input" placeholder="select date">
                                </div>

                            </div>
                            <div class="col-md-6 ">
                                <p>
                                    Items or Items Group Name/No


                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select</option>

                                        <option value="AK">EMS Unit</option>
                                        <option value="AK">Shoulder Support</option>
                                        <option value="AK">Orthopedic Car Seat</option>



                                    </select>
                                </div>

                            </div>

                        </div>
                        <div class="row">

                            <div class=" col-md-6 ">
                                <p>CPT Code</p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select</option>

                                        <option value="AK">100</option>
                                        <option value="AK">1002</option>
                                        <option value="AK">2001</option>



                                    </select>
                                </div>
                            </div>
                            <div class="col-md-6 ">
                                <p>
                                    ICD Code

                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select</option>

                                        <option value="AK">100</option>
                                        <option value="AK">1002</option>
                                        <option value="AK">2001</option>



                                    </select>
                                </div>

                            </div>

                        </div>
                        <div class="row">
                            <div class=" col-md-6 ">
                                <p>Prescriber</p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select</option>

                                        <option value="AK">Dr Asad</option>



                                    </select>
                                </div>
                            </div>
                            <div class="col-md-6 ">
                                <p>
                                    Barcode No

                                </p>


                                <div class="input-field">
                                    <input type="text" class="text-input" placeholder="Enter Barcode">
                                </div>


                            </div>


                        </div>
                        <div class="row">
                            <div class=" col-md-6 ">
                                <p>Prescription Refference/No</p>

                                <div class="input-field">
                                    <input type="text" class="text-input" placeholder="Enter Prescription No">
                                </div>
                            </div>
                            <div class="col-md-6 ">
                                <p>
                                    Upload Document

                                </p>


                                <input type="file" id="input-file-now-custom-2" class="dropify" data-height="100" />


                            </div>


                        </div>
                        <div class="row mt-2 mb-2">
                            <div class="col s12">
                                <div class="center">
                                    <a href="javascript:void(0);" class="waves-effect waves-light btn add-btn">Save</a>
                                    <a href="javascript:void(0);" class="waves-effect waves-light Recreate btn">Edit</a>
                                    <a href="javascript:void(0);" class="waves-effect waves-light Recreate btn">Cancel</a>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            
        </section>

    </div>
</div>





