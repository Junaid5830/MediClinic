        $('#smsForm').on('submit', function (e) {
            debugger;
            $('#phoneNoField').attr('disabled', false);
            e.preventDefault();

            if ($(this).valid()) {
                $(this).ajaxSubmit({
                    success: function (response) {
                    onSendSmsSuccess(response);
                    },
                    error: function (response) {

                    },
                    complete: function (xhr) {

                    }
                });

            }
            else {
                $('#phoneNoField').attr('disabled', true);
            }
        });
        function onSendSmsSuccess(data) {
            $('#phoneNoField').val('');
            $('#msgTxt').val('');
            $('#myModal').modal('hide');

            if (data) {
                $(".checkmark").show().fadeOut(6000);

            }
            else {
                new PNotify({
                    title: 'Error!',
                    text: 'Something went wrong',
                    delay: 2500,
                    type: 'error',
                    addclass: 'pnotify-center'
                });
            }
        }
        function importNumber() {
            debugger;
             $('#phoneNoField').attr('disabled', true);
            $('.removeValidation').html("");
            var phone = $('#phoneNo').val();
            if (phone) {
                $('#phoneNoField').val(phone);
                $('#phoneNoField').attr('disabled', true);
            }
        }
        function importEmail() {
            $("#email").attr('disabled', true);
            $('.removeValidation').html("");
            var email = $('#PatientEmail').val();
            if (email) {
                $('#email').val(email);
            }
        }
        $('#EmailForm').on('submit', function (e) {
            debugger;
            $("#email").attr('disabled', false);
            e.preventDefault();
                        debugger;

            if ($(this).valid()) {
                $('#emailbtn').attr('disabled', true);
                $(this).ajaxSubmit({
                    success: function (response) {
                        $('#emailbtn').attr('disabled', false);
                    },
                    error: function (response) {
                        $('#emailbtn').attr('disabled', false);

                    },
                    complete: function (xhr) {
                        $('#emailbtn').attr('disabled', false);

                        onEmailSendSuccess(xhr.responseJSON);
                        //console.log(xhr.responseText);
                    }
                });

            }
            else {
                $("#email").attr('disabled', true);
            }
        });
        function onEmailSendSuccess(data) {
            $('#email').val('');
