
@{
    ViewData["Title"] = "ClaimInfolist";
    //Layout = "~/Views/Shared/_PatientLayout/_PatientLayout.cshtml";
    Layout = "~/Views/Shared/_LayoutElite.cshtml";

}

@{
    var NewId = ViewContext.RouteData.Values["id"];

}
@section styles{


    <style>


        .dropdown-select {
            background-color: #fff !important;
            color: #6b6f82 !important;
            border: 1px solid #6b6f82 !important;
            margin-top: 0px !important;
            padding: 5px 1rem;
            border-radius: 4px;
            height: 41px;
            line-height: 29px;
            box-shadow: 0px 0px 3px #6b6f82;
        }

        .search-icons {
            left: 12px;
            top: 9px;
        }

        .position-relative {
            position: relative;
        }

        .dropbody {
            margin-top: 40px !important;
        }

        .card {
            margin: 1rem 0 0rem 0 !important;
        }
    </style>
}
@section scripts{
    <script>

        $(document).ready(function () {


            $("#Claiminfo-table").DataTable({ "info": false, responsive: true, lengthMenu: [[10, 25, 50, -1], [10, 25, 50, "All"]], 'order': [], 'columnDefs': [{ "targets": [0,9], "orderable": false }] });

        });
        function myFillNowFunction() {
            debugger;
            var route = "@Url.Action("PatientInfo", "PatientSide", new {id=NewId})";
            //var route = "/" + controller + "/" + action + "/" + patientId;
            var Tabname = "#test6";
            var uri = route + decodeURIComponent(Tabname);
            window.location.href = uri;
        }

    </script>
}
<div class="row">
    <div class="col s12">
        <div class="container">

            <div class="card">
                <div class="card-content">

                    <div class="row">
                        <div class="col s4 m4 l3">
                            <h6>Total Billed</h6>
                            <p>$300,000.00</p>
                        </div>
                        <div class="col s4 m4 l6">
                            <h6>Total Paid</h6>
                            <p>$500.000</p>
                        </div>
                        <div class="col s4 m4 l3">
                            <h6>Outstanding</h6>
                            <p style="color: #f04f85 !important;">$200,000.00</p>

                        </div>

                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
<div id="breadcrumbs-wrapper">
    <!-- Search for small screen-->
    <div class="container">
        <div class="row">
            <div class="col s3 m3 l2 margin-top-8">
                <h5 class="breadcrumbs-title color-blue">Claim Info</h5>
            </div>
            <div class="col s9 m5 l6 display-block">
                <div class="header-search-wrapper  display-flex position-relative">
                    <i class="material-icons position-absolute  search-icons">search</i>
                    <input class="header-search-input z-depth-2 search-box" type="text" data-search placeholder="Search">
                    <a class="btn ml-2 search-btn">
                        Search
                    </a>
                </div>
            </div>
            <div class="col s12 m4 l4 ">

                <a class="btn  float-right add-btn" onclick="myFillNowFunction();">
                    Add New Claim
                </a>
            </div>

        </div>
    </div>
</div>

