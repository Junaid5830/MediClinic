


@{
    ViewData["Title"] = "AddIMEElite";
    Layout = "~/Views/Shared/_LayoutElite.cshtml";
}


@section styles{
 
   
}
@section scripts{


    <script>
        $(function () {
            $(".select2").select2();
            $('.selectpicker').selectpicker();
        });


    </script>
   
}
<div class="page-wrapper">
    <div class="container">
        <div id="breadcrumbs-wrapper">
            <!-- Search for small screen-->
           
                <div class="row">
                    <div class=" col" margin-top-8">
                        <h5 class="breadcrumbs-title color-blue"> Immunizations</h5>
                    </div>

                    <div class="col">
                        <a class="waves-effect waves-light float-right" style="margin-top:-11px" href="/patientSideElite/ImmunizationElite">
                            <i class="mdi mdi-arrow-left right arrow-icon"></i>

                        </a>
                    </div>

                </div>
            
        </div>
     
            
                <div class="row">
                    <form class="col-sm-12  ">
                        <div class="row">
                            <div class=" col-md-6">
                                <p>Date of Immunizaton</p>

                                <div class="input-field">
                                    <input type="date" class="text-input" placeholder="select date">
                                </div>
                            </div>
                            <div class=" col-md-6 ">
                                <p>Time</p>

                                <div class="input-field">
                                    <input type="time" class="text-input" placeholder="select time">
                                </div>
                            </div>

                        </div>
                        <div class="row">

                            <div class="col-md-6 ">
                                <p>
                                    Examiner
                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select Examiner</option>

                                        <option value="AK">Bronx NY</option>
                                        <option value="HI">New Jersy</option>
                                      </select>
                                </div>
                             </div>
                       <div class="col-md-6 ">
                                <p>
                                    Type

                                </p>
                               <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select Type</option>

                                        <option value="AK">Smart Guy, Esq</option>
                                        <option value="HI">Rosy</option>
                                    </select>
                                </div>
                           </div>
                      </div>
                        <div class="row">
                            <div class="col-md-6 ">
                                <p>
                                    Reason
                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select Reason</option>

                                        <option value="AK">N/A</option>



                                    </select>
                                </div>

                            </div>
                            <div class="col-md-6 ">
                                <p>
                                   Results
                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select Result</option>
                                        <option value="AK">Yes</option>
                                        <option value="AK">No</option>

                                  </select>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6 ">
                                <p>
                                    Report
                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select Report</option>

                                        <option value="AK">No show</option>
                                       </select>
                                </div>
                                     </div>
                            <div class="col-md-6 ">
                                <p>
                                    Signed
                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select Signed</option>

                                        <option value="AK">No show</option>

                                    </select>
                                </div>
                           </div>
                        </div>
                        <div class="row mt-2 mb-2">
                            <div class="col s12">
                                <div class="text-right">
                                    <a href="javascript:void(0);" class="waves-effect waves-light btn add-btn">Save</a>

                                </div>
                            </div>
                        </div>
                    </form>
                </div>
          
        

    </div>
</div>




