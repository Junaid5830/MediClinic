        <div class="row tablee">
            <div class="col-md-12">
                <div class="table-responsive">
                    <table id="VitalTab" class="table dataTables_wrapper ">
                        <thead>
                            <tr class="uppercase">
                                <th class="disable">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input select-all" data-val="all" id="selectAll">
                                        <label class="custom-control-label" for="selectAll"></label>
                                    </div>
                                </th>
                                <th>Vaccine</th>
                                <th>Abbreviations</th>
                                <th>Age</th>
                                <th>Dose</th>
                                <th>Status</th>
                                @if (!(RoleNames.Patient.Equals(userRole.ToString())))
                                {
                                    <th data-orderable="false" class="all">ACTION  </th>

                                }
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.Listimmunization)
                            {
                            <tr class="card" style="cursor: pointer">
                                <th>
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" id="@item.ImmunizationId" value="check">
                                        <label class="custom-control-label" for="@item.ImmunizationId"></label>
                                    </div>
                                </th>
                                <td>@item.VaccineName</td>
                                <td>@item.VaccineAbberivation</td>
                                <td>@item.PatientCurrentAge.Value.ToString("MM/dd/yyyy")</td>
                                <td>@item.DoesInRouten</td>

                                @if (item.RouteOfAdministration < Date)
                                {
                                    <td>Past</td>
                                }
                                else if (item.RouteOfAdministration > Date)
                                {
                                    <td>Future</td>
                                }
                                else
                                {
                                    <td>Current</td>
                                }
                                @if (!(RoleNames.Patient.Equals(userRole.ToString())))
                                {
                                    <td class="action">
                                        <button class="btn btn-primary dropdown-toggle table_btn" type="button" data-toggle="dropdown">
                                            Options
                                            @*<span class="caret"></span>*@
                                        </button>
                                        <ul class="dropdown-menu Table_Drop ul_Caim">
                                            <li><a href="@Url.Action("AssignImmunization","Immunization",new { Id = item.ImmunizationId})">Edit</a></li>
                                            <li><a href="javascript:void(0);">View</a></li>
                                            <li><a onclick="ImmuPatient(@item.ImmunizationId, this);">Delete</a></li>
                                        </ul>
                                    </td>
                                }

                            </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
