@section styles
{
<style>.card .card-content {
    padding: 8px;
    border-radius: 0 0 2px 2px;
    }
hr
{
        border-top: 8px solid #177e64;
    border-radius: 10px;
}
.smallCap
{
    border: 2px solid #177e64;
    background-color: #167f64;
    border-radius: 25px;
    /* padding: 5px; */
    width: 73px;
    text-align: center;
    height: 20px;
    /* margin-top: 3px; */
