

@{
    ViewData["Title"] = "AddNewElite";
    Layout = "~/Views/Shared/_LayoutElite.cshtml";
}


@section styles{
  
}
@section scripts{
  
    <script>
        $(function () {

            $(".select2").select2();
            $('.selectpicker').selectpicker();
        });


    </script>
    <script>
	$(document).ready(function () {
		$('.dropify').dropify();
	});
    </script>
}
<div class="page-wrapper">
    <div class="container">
        <div id="breadcrumbs-wrapper">
           
                <div class="row">
                    <div class=" col" margin-top-8">
                        <h5 class="breadcrumbs-title color-blue"> I.M.E</h5>
                    </div>

                    <div class="col">
                        <a href="@Url.Action("IME","provider")" class="waves-effect waves-light float-right" style="margin-top:-11px">
                            <i class="mdi mdi-arrow-left right arrow-icon"></i>

                        </a>
                    </div>

                </div>
            
        </div>
   
                <div class="row">
                    <form class="col-sm-12  ">
                        <div class="row">
                            <div class=" col-sm-6 col-md-6 col-xl-6 col-12">
                                <p>Date of IME</p>

                                <div class="input-field">
                                    <input type="date" class="text-input" placeholder="select date">
                                </div>
                            </div>
                            <div class=" col-sm-6 col-md-6 col-xl-6 col-12">
                                <p>Time of IME</p>

                                <div class="input-field">
                                    <input type="time" class="text-input" placeholder="select time">
                                </div>
                            </div>

                        </div>
                        <div class="row">

                            <div class=" col-sm-6 col-md-6 col-xl-6 col-12">
                                <p>
                                    Place of EUO

                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select place</option>

                                        <option value="AK">Bronx NY</option>
                                        <option value="HI">New Jersy</option>


                                    </select>
                                </div>

                            </div>

                            <div class=" col-sm-6 col-md-6 col-xl-6 col-12">
                                <p>
                                    Represented By

                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select Representative</option>

                                        <option value="AK">Smart Guy, Esq</option>
                                        <option value="HI">Rosy</option>


                                    </select>
                                </div>

                            </div>

                        </div>
                        <div class="row">
                            <div class=" col-sm-6 col-md-6 col-xl-6 col-12">
                                <p>
                                    Status of EUO
                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select Status</option>
                                        <option value="AK">Yes</option>
                                        <option value="AK">No</option>



                                    </select>
                                </div>

                            </div>

                            <div class=" col-sm-6 col-md-6 col-xl-6 col-12">
                                <p>
                                    Reason
                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select Reason</option>

                                        <option value="AK">N/A</option>



                                    </select>
                                </div>

                            </div>
                        </div>
                        <div class="row">
                            <div class=" col-sm-6 col-md-6 col-xl-6 col-12">
                                <p>
                                    Transcripts
                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select Transcripts</option>

                                        <option value="AK">No show</option>



                                    </select>
                                </div>

                            </div>
                        </div>
                        <div class="row mt-2 mb-2">
                            <div class="col s12">
                                <div class="center">
                                    <a href="javascript:void(0);" class="waves-effect waves-light btn add-btn">Save</a>
                                    <a href="#" class=" btn  Recreate">
                                        Edit
                                    </a>
                                    <a href="" class="Recreate btn">Cancel</a>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
          
    </div>
</div>





