<div class="container">
    <div class="row">
        <div class="col s12">
            <button class="add-btn btn right mb-1" id="lab_btn">Add New Lab</button>
        </div>
    </div>
</div>
<div class="container" id="lab_form" style="display:none">
    <div class="row">
        <form class="form-inline col s12">
            <div class="row">
                <div class="col s12 m6">
                    <div class="input-field pl-0">
                        <p>Date/Time Of Exam</p>
                        <input type="text" class="datepicker" placeholder="Select Date">
                    </div>
                </div>
                <div class="col s12 m6">
                    <div class="input-field pl-0">
                        <p>Examiner</p>
                        <input class="text-input" type="text" placeholder="Enter Examiner Name">
                    </div>

                </div>
            </div>
            <div class="row">
                <div class=" col s12 m6">
                    <div class="input-field pl-0">
                        <p>Type of Exam</p>
                        <input class="text-input" type="text" placeholder="Enter Type of Exam">
                    </div>
                </div>
                <div class=" col s12 m6">
                    <div class="input-field pl-0">
                        <p>Reason for Exam</p>
                        <input class="text-input" type="text" placeholder="Enter Reason for Exam">
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col s12 m6">
                    <p>Results</p>
                    <div class="input-field pl-0">
                        <input class="text-input" type="text" placeholder="Enter Results">
                    </div>
                </div>
                <div class="col s12 m6">
                    <div class="input-field pl-0">
                        <p>Report</p>   
                        <input class="text-input" type="text" pattern="Enter Report">
                    </div>
                </div>
            </div>
            <div class="row mt-2 mb-2">
                <div class="col s12 m12" style="text-align:center">
                    <a href="javascript:void(0);" class="waves-effect waves-light btn add-btn">Save</a>
                </div>
            </div>

        </form>
    </div>
</div>
