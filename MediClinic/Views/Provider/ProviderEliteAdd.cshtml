        function checkTimeConflictsWithOthers(stTime, enTime, nextStTime, nextEnTime) {
            debugger
            var time_start = new Date();
            var time_end = new Date();
            var nextTime_start = new Date();
            var nextTime_end = new Date();


            var value_start = stTime.split(':');
            var value_end = enTime.split(':');

            var currentValue_start = nextStTime.split(':');
            var currentValue_end = nextEnTime.split(':');

            time_start.setHours(value_start[0], value_start[1], 0)
            time_end.setHours(value_end[0], value_end[1], 0)

            nextTime_start.setHours(currentValue_start[0], currentValue_start[1], 0)
            nextTime_end.setHours(currentValue_end[0], currentValue_end[1], 0)


            if (nextTime_start > time_start && nextTime_start < time_end) {
                return true
            }
            else {
                return false;
            }

        }

        function checkTimeConflicts(stTime, enTime) {
            debugger
            var time_start = new Date();
            var time_end = new Date();
            var value_start = stTime.split(':');
            var value_end = enTime.split(':');

            time_start.setHours(value_start[0], value_start[1], 0)
            time_end.setHours(value_end[0], value_end[1], 0)

            var result = time_end - time_start;
            if (result > 0) {
                return false;
            }
            else {
                return true
            }
        }

