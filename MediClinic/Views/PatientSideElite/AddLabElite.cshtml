    Layout = "~/Views/Shared/_LayoutElite.cshtml";
}


@section styles{

   
}
@section scripts{
  

    <script>
        $(function () {

         
            $('.selectpicker').selectpicker();
        });


    </script>
   
}
<div class="page-wrapper">
    <div class="container">
        <div id="breadcrumbs-wrapper">
            <!-- Search for small screen-->
  
                <div class="row">
                    <div class=" col" margin-top-8">
                        <h5 class="breadcrumbs-title color-blue"> New Lab</h5>
                    </div>

                    <div class="col">
                        <a class="waves-effect waves-light float-right" style="margin-top:-11px"  href="/patientSideElite/LabsElite">
                            <i class="mdi mdi-arrow-left right arrow-icon"></i>

                        </a>
                    </div>

                </div>
        </div>
       
                <div class="row">
                    <form class="col-sm-12  ">
                        <div class="row">
                            <div class=" col ">
                                <p>Date/Time of exam</p>

                                <div class="input-field">
                                    <input type="date" class="text-input" placeholder="select date">
                                </div>
                            </div>
                            <div class=" col ">
                                <p>Examiner</p>

                                <div class="input-field">
                                    <input type="text" class="text-input" placeholder="Enter Examiner Name">
                                </div>
                            </div>

                        </div>
                        <div class="row">

                            <div class=" col ">
                                <p>Type of Exam</p>

                                <div class="input-field">
                                    <input type="text" class="text-input" placeholder="Enter Type of Exam">
                                </div>
                            </div>
                            <div class=" col ">
                                <p>Reason for exam</p>

                                <div class="input-field">
                                    <input type="text" class="text-input" placeholder="Enter reason for exam">
                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class=" col ">
                                <p>Results</p>

                                <div class="input-field">
                                    <input type="text" class="text-input" placeholder="Enter Results">
                                </div>
                            </div>
                            <div class=" col ">
                                <p>Report</p>

                                <div class="input-field">
                                    <input type="text" class="text-input" placeholder="Enter Report">
                                </div>
                            </div>

                        </div>
                        <div class="row mt-2 mb-2">
                            <div class="col s12">
                                <div class="center">
                                    <a href="javascript:void(0);" class="waves-effect waves-light btn add-btn">Save</a>
                                   
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
       
    </div>





