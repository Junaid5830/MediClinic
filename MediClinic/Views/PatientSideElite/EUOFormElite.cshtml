
@{
    ViewData["Title"] = "EUOFormElite";
    Layout = "~/Views/Shared/_LayoutElite.cshtml";
}





@section styles{

    <style>


        .EUOElite p {
            margin-top: 1rem !important;
        }
    </style>

}
@section scripts{

    <script>
        $(function () {

            $(".select2").select2();
            $('.selectpicker').selectpicker();
        });


    </script>



}
<div class="page-wrapper">
    <div class="container">
        <div id="breadcrumbs-wrapper">
            <!-- Search for small screen-->
       
                <div class="row">
                    <div class=" col" margin-top-8">
                        <h5 class="breadcrumbs-title color-blue">EUO</h5>
                    </div>

                    <div class="col">
                        <a class="waves-effect waves-light float-right" style="margin-top: -11px" href="/patientSideElite/EuoElite">
                            <i class="mdi mdi-arrow-left right arrow-icon"></i>

                        </a>
                    </div>

                </div>
         
        </div>
                <div class="row">
                    <form class="col-sm-12  ">
                        <div class="row">
                            <div class="col-md-6">
                                <p>
                                    Date of Euo

                                </p>

                                <div class="input-field">
                                    <input type="date" class="text-input" placeholder="select date">
                                </div>

                            </div>
                            <div class="col-md-6 ">
                                <p>
                                    Time of Euo

                                </p>

                                <div class="input-field">

                                    <input type="time" class="text-input" >
                                </div>

                            </div>

                        </div>
                        <div class="row">

                            <div class=" col-md-6 ">
                                <p>Place Of Euo</p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select</option>

                                        <option value="AK">NY</option>



                                    </select>
                                </div>
                            </div>
                            <div class="col-md-6 ">
                                <p>
                                    Represented By

                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select</option>

                                        <option value="AK">Smart Guy</option>



                                    </select>
                                </div>

                            </div>

                        </div>
                        <div class="row">
                            <div class=" col-md-6 ">
                                <p>
                                    Status Of EUO

                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select</option>

                                        <option value="AK">No Show</option>



                                    </select>
                                </div>
                            </div>
                            <div class="col-md-6 ">
                                <p>
                                  Reason

                                </p>


                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select</option>

                                        <option value="AK">None</option>



                                    </select>
                                </div>


                            </div>


                        </div>
                        <div class="row">
                            <div class=" col-md-6 ">
                                <p>Transcripts</p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select</option>

                                        <option value="AK">Yes</option>
                                        <option value="AK">No</option>
                                   


                                    </select>
                                </div>
                            </div>
                          

                        </div>
                        <div class="row mt-2 mb-2">
                            <div class="col s12">
                                <div class="center">
                                    <a href="javascript:void(0);" class="waves-effect waves-light btn add-btn">Save</a>
                                    <a href="javascript:void(0);" class="waves-effect waves-light Recreate btn">Edit</a>
                                    <a href="javascript:void(0);" class="waves-effect waves-light Recreate btn">Cancel</a>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
    </div>
</div>







