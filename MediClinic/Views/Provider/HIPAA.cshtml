    Layout = "~/Views/Shared/_LayoutElite.cshtml";
}

@section styles
{

    

}
@section scripts{
    
    <script>
        $(function () {

            $(".select2").select2();
            $('.selectpicker').selectpicker();
		});


    </script>

}
<div class="page-wrapper">
    <div class="container">

        <div id="breadcrumbs-wrapper">
            <div class="row">
                <div class="col-sm-5 col-md-3 col-lg-3" margin-top-8">
                    <h5 class="breadcrumbs-title color-blue">
                        Hipaa Release
                    </h5>
                </div>
                <div class="col-sm-7 col-md-6 col-lg-6" display-block">

                    @*<div class="input-group-prepend">
                                                        <span class="input-group-text" id="basic-addon11" style="		background-color: #fff !important;
                        "></span>
                                                    </div>*@ <div class="header-search-wrapper  display-flex position-relative">
                        <i class="ti-search position-absolute  search-icons"></i>
                        <input type="text" class="form-control set_Place search-box z-depth-2 search_box2" placeholder="Search" aria-label="Username" aria-describedby="basic-addon11">
                        <a class="btn ml-2 search-btn">
                            Search
                        </a>
                    </div>
                </div>
                <div class="col-sm-12 col-md-3 col-lg-3">

                    <a class="btn  float-right add-btn set_smallscreen">
                        Add New Hipaa
                    </a>
                </div>

            </div>
        </div>


        <div class="row">
            <div class="col-sm-12 col-md-4 col-lg-4  col-12">
                <p>Choose Hipaa State</p>
                <select class="select2 form-control  text-input" style="width: 100%;">
                    <option>Select</option>

                    <option value="AK">Dr.Aibolit Template</option>
                    <option value="HI">Dr.John</option>


                    <option value="CA">Dr.Brown</option>

                </select>
            </div>
            <div class="col-sm-12 col-md-4 col-lg-4   col-12">
                <p class="P_set">Open Prefilled Form</p>
                <select class="select2 form-control  text-input" style="width: 100%;">
                    <option>Select Prefilled Form</option>

                    <option value="AK">Option 1</option>
                    <option value="HI">Option 2</option>


                    <option value="CA">Option 3</option>

                </select>
            </div>
            <div class="col-sm-12 col-md-4 col-lg-4  col-12 ">
                <p class="P_set">Open Blank Form</p>
                <select class="select2 form-control  text-input" style="width: 100%;">
                    <option>Open Blank Form</option>

                    <option value="AK">Option 1</option>
                    <option value="HI">Option 2</option>


                    <option value="CA">Option 3</option>

                </select>
            </div>
        </div>
        <div class="row mt-3 mb-3">
            <div class="col s12 m12" style="text-align:center">
                <a href="javascript:void(0);" class="waves-effect waves-light btn  add-btn">Save</a>
                <a href="javascript:void(0);" class="waves-effect waves-light Recreate btn ">Edit</a>
                <a href="javascript:void(0);" class="waves-effect waves-light Recreate btn ">Cancel</a>
            </div>

        </div>
