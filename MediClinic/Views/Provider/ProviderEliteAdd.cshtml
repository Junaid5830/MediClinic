        function checkIsDuplicateEntry(stTime, enTime,DivName) {
            var isExist = false;
            var existCounter = 0;

            if (stTime && enTime) {
                $("#" + DivName + " .tr").each(function (index, value) {
                    debugger
                    $(this).find('.td').each(function (i, v) {
                        debugger
                        endTime = $(this).closest('div').parent().next().find('input').val();

                        if ($(this).attr('name') == 'StartTime' && ($(this).val() == stTime)) {
                            existCounter++;
                        }
                        else if ($(this).attr('name') == 'StartTime' && endTime) {
                            var status = checkTimeConflictsWithOthers(stTime, enTime, $(this).val(), endTime);
                            if (status) {
                                existCounter++;
                            }
                        }
                    });
                })
            }

            else if (stTime) {
                $("#" + DivName + " .tr").each(function (index, value) {
                    debugger
                    $(this).find('.td').each(function (i, v) {
                        debugger
                        //var convertedVal =
                        if ($(this).attr('name') == 'StartTime' && $(this).val() == stTime) {
                            existCounter++;
                        }
                    });
                })
            }
            else if (enTime) {
                $("#" + DivName + " .tr").each(function (index, value) {
                    $(this).find('.td').each(function (i, v) {
                        debugger

                        if ($(this).attr('name') == 'EndTime' && $(this).val() == enTime) {
                            existCounter++;
                        }

                    });
                })
            }
            if (existCounter > 1) {
                isExist = true;
            }

            return isExist;
        }

