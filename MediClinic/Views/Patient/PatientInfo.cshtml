
@{
    ViewData["Title"] = "PatientInfo";
    //Layout = "~/Views/Shared/_PatientLayout/_PatientLayout.cshtml";
    Layout = "~/Views/Shared/_LayoutElite.cshtml";

}

@section styles{
    <style>
        .sidebar .sidebar-content .sidebar-header {
            display: none !important;
        }

        .font-size-1 {
            font-size: 1.5rem !important;
        }

        @@media only screen and (max-width:900px) {
            .card {
                margin-top: 50px !important;
            }
        }

        .set_formadd {
            margin-top: 22px !important;
        }



        #slide-out-right3.sidenav {
            top: 66px;
            width: 340px;
            padding-bottom: 0;
        }

        #slide-out-right4.sidenav {
            top: 66px;
            width: 340px;
            padding-bottom: 0;
        }
    </style>
}
@section scripts{
    <script>
        $(document).ready(function () {
              $(".ui-menu ul").append("<li>mr2</li>");
            // INIT AUTOCOMPLETE
            $('.prefix').autocomplete({
                data: {
                    "Mr": null,
                    "Miss": null,
                    "Mrs": null,
                    "Master": null,
                },
                limit: 10,
                minLength: 0
            });

            $('.suffix').autocomplete({
                data: {
                    "Jr": null,
                    "Sr": null,
                },
                limit: 10,
                minLength: 0
            });
            $("li.patientheader").addClass("border-bottom-header-active");

        });

        $('.gender').autocomplete({
                data: {
                    "Male": null,
                    "Female": null,
                    "Other": null,
                },
                limit: 10,
                minLength: 0
        });

        $('.dateOfDOB').autocomplete({
                data: {
                    "1": null,
                    "2": null,
                    "3": null
                },
                limit: 10,
                minLength: 0
            });

            $('.monthOfDOB').autocomplete({
                data: {
                    "January": null,
                    "Febrary": null,
                    "March": null,
                    "April": null,
                },
                limit: 10,
                minLength: 0
            });

            $('.yearOfDOB').autocomplete({
                data: {
                    "2016": null,
                    "2017": null,
                    "2018": null,
                    "2019": null,
                },
                limit: 10,
                minLength: 0
            });

            $('.maritalStatus').autocomplete({
                data: {
                    "Married": null,
                    "Single": null,

                },
                limit: 10,
                minLength: 0
            });

         $('.raceEthnicity').autocomplete({
                data: {
                    "Option1": null,
                    "Option2": null,
                    "Option3": null,
                    "Option4": null,
                },
                limit: 10,
                minLength: 0
         });

        $('.state').autocomplete({
                data: {
                    "Falorida": null,
                    "California": null,
                    "Texas": null,
                },
                limit: 10,
                minLength: 0
        });

        $('.patientTreatmentStatus').autocomplete({
                data: {
                    "Active": null,
                    "Inactive": null,
                    "Disable": null,
                },
                limit: 10,
                minLength: 0
        });

        $('.patientLegalStatus').autocomplete({
                data: {
                    "Active": null,
                    "Inactive": null,
                    "Disable": null,
                },
                limit: 10,
                minLength: 0
            });
    </script>

}

