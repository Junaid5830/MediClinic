
@{
    ViewData["Title"] = "Missing";
    Layout = "~/Views/Shared/_PatientLayout.cshtml";
}
@section scripts{
    <script src="~/app-assets/js/scripts/ui-chips.min.js"></script>
}
@section styles{
    <style>
        .sidebar .sidebar-content .sidebar-header {
            display: none !important;
        }

        p {
            margin: 0 auto !important;
        }

        .font-size-1 {
            font-size: 1.5rem !important;
        }




        .set_formadd {
            margin-top: 22px !important;
        }

        .set_rowbtn {
            margin: 71px auto !important;
        }

        .divider {
            height: unset;
        }
    </style>
}

<div id="breadcrumbs-wrapper">
    <!-- Search for small screen-->
    <div class="container">
        <div class="row">
            <div class="col s6 m6 ">
                <h5 class="breadcrumbs-title color-blue mt-0">Missing</h5>
            </div>

            @*<div class="col s6 m6 ">
                    <a class="waves-effect waves-light float-right mr-2" style="margin-top: -1px;" href="/Home/visits">
                        <i class="material-icons  right arrow-icon">arrow_back</i>

                    </a>
                </div>*@
        </div>
    </div>
</div>
<div class="container">
    <div class="row">
        <form class="form-inline col s12">
            <div class="row">

                <div class="col s12 m6">
                    <p class="mb-0">

                        Photo I.D
                    </p>
                    <div class="input-field ">
                        <input class="text-input input-styling " type="text" placeholder="Enter Photo ID">
                    </div>
                </div>
                <div class="col s12 m6">
                    <p class="mb-0">
                        Signature
                    </p>
                    <div class="input-field">
                        <input class="text-input input-styling " type="text" placeholder="Enter signature">
                    </div>
                </div>

            </div>
            <div class="row">
                <div class="col s12 m6">
                    <p class="mb-0">
                        Phone Number
                    </p>
                    <div class="input-field">
                        <input class="text-input input-styling " type="text" placeholder="Enter Phone Number">
                    </div>

                </div>
                <div class="col s12 m6">
                    <p class="mb-0">
                        SS No.
                    </p>
                    <div class="input-field">
                        <input class="text-input input-styling " type="text" placeholder="Enter SS No.">

                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col s12 m6">
                    <p class="mb-0">

                        P.I Attorney Info
                    </p>
                    <div class="input-field">
                        <input class="text-input input-styling " type="text" placeholder="Enter P.I. ATTORNEY INFO">
                    </div>

                </div>
                <div class="col s12 m6">
                    <p class="mb-0">

                        Collection Attorney Info
                    </p>
                    <div class="input-field">
                        <input class="text-input input-styling " type="text" placeholder="Enter COLLECTION ATTORNEY INFO">

                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col s12 m6">

                    <p>
                        NF2 Filing Date
                    </p>
                    <div class="input-field">
                        <input type="text" class="datepicker" placeholder="Select Date">
                    </div>

                </div>


                <div class="col s12 m6">
                    <p class="mb-0">

                        Medication
                    </p>
                    <div class="input-field">
                        <input class="text-input input-styling " type="text" placeholder="Enter MEDICATION">

                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col s12 m6">
                    <p class="mb-0">
                        DME
                    </p>
                    <div class="input-field">
                        <input class="text-input input-styling " type="text" placeholder="Enter DME">
                    </div>

                </div>
                <div class="col s12 m6">
                    <p class="mb-0">
                        Rx
                    </p>
                    <div class="input-field">
                        <input class="text-input input-styling " type="text" placeholder="Enter Rx">

                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col s12 m6">
                    <p class="mb-0">
                        EMG
                    </p>
                    <div class="input-field">
                        <input class="text-input input-styling " type="text" placeholder="Enter EMG">
                    </div>

                </div>
                <div class="col s12 m6">
                    <p class="mb-0">

                        Range Of Motion Test
                    </p>
                    <div class="input-field">
                        <input class="text-input input-styling " type="text" placeholder="Enter RANGE OF MOTION TEST ">

                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col s12 m6">
                    <p class="mb-0">
                        Fanctional Capacity Test
                    </p>
                    <div class="input-field">
                        <input class="text-input input-styling " type="text" placeholder="Enter FANCTIONAL CAPACITY TEST">
                    </div>

                </div>
                <div class="col s12 m6">
                    <p class="mb-0">

                        Inital Exam
                    </p>
                    <div class="input-field">
                        <input class="text-input input-styling " type="text" placeholder="Enter INITIAL EXAM ">

                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col s12 m6">
                    <p class="mb-0">
                        MRI
                    </p>
                    <div class="input-field">
                        <input class="text-input input-styling " type="text" placeholder="Enter MRI">
                    </div>

                </div>
            </div>
            <div class="row">
                <div class="col s12 m12" style="text-align:center">
                    <a href="javascript:void(0);" class="btn add-btn">Save</a>
                    <a href="javascript:void(0);" class="Recreate btn">Edit</a>
                    <a href="javascript:void(0);" class="Recreate btn">Cancel</a>
                </div>
            </div>



        </form>

    </div>
    <div class="divider mt-2"></div>
</div>

