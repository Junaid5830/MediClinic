@model MediClinic.Models.TemplateViewModal
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
@if (!(Model.TemplateList is null))
{
    @foreach (var item in Model.TemplateList)
    {
        <div class="row tablee">
            <div class="col-md-12">

                <h6 class="color-blue">Note Name : @item.Name </h6>
            </div>
        </div>
        <div class="row">
            @foreach (var loop in item.VisitDetails)
            {
                <div class="col-md-3">
                    <div class="card">
                        <div class="card-body">
                            <p><span class="color-blue">@loop.Label</span> : <span style="color: #6b6f82 !important">@loop.Value</span></p>
                        </div>
                    </div>
                </div>
            }

        </div>
    }
}
else
{
    <div class="row">
        <div class="col-12">
            <span class="center">No data available</span>

        </div>
    </div>
}