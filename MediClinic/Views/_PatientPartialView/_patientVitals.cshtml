<div class="container">
    <div class="row">
        <div class="col s12">
            <h6 class="color-blue">Past Vitals</h6>
            <table id="Pastlablist" class="display lablist">
                <thead>
                    <tr class="uppercase">
                        <th>
                            <label>
                                <input type="checkbox" class="Pastlablist" />
                                <span></span>
                            </label>
                        </th>
                        <th>
                            DATE/TIME OF EXAM
                        </th>
                        <th>
                            EXAMINER
                        </th>
                        
                        <th>
                            REASON FOR EXAM
                        </th>
                        <th>RESULTS</th>
                        
                        <th class="all">Actions</th>

                    </tr>
                </thead>
                <tbody>
                    <tr class="card" style="cursor: pointer">
                        <th>
                            <label>
                                <input type="checkbox" />
                                <span></span>
                            </label>
                        </th>
                        <td>
                            12/5/2018
                        </td>
                        <td>
                            Dr Aibolit
                        </td>
                        <td>
                            REQUESTED
                        </td>
                        <td>
                          Temp: 98.00 F (36.67 c)
                        </td>

                        <td>
                            <a class='dropdown-trigger btn action-btn option-dropdown-btn' href='javascript:void(0);' data-target='dropdown1'>Options <i class="material-icons right arrow-set">keyboard_arrow_down</i></a>
                            <ul id='dropdown1' class='dropdown-content'>
                                <li><a href="#!" class="option-a"><i class="material-icons">remove_red_eye</i>Preview</a></li>
                                <li><a href="#!" class="option-a"><i class="material-icons">print</i>Print</a></li>
                                <li><a href="#!" class="option-a"><i class="material-icons">share</i>Share</a></li>

                            </ul>

                        </td>

                    </tr>
                </tbody>
            </table>
            <a href="@Url.Action("Labs","Patient")" class="right exam_viewmore">View More</a>
            <h6 class="color-blue">Current Vitals</h6>
            <table id="Currentlablist" class="display lablist">
                <thead>
                    <tr class="uppercase">
                        <th>
                            <label>
                                <input type="checkbox" class="Currentlablist" />
                                <span></span>
                            </label>
                        </th>
                        <th>
                            DATE OF EXAM
                        </th>
                        <th>
                            EXAMINER
                        </th>
                        <th>
                            REASON FOR EXAM
                        </th>
                        <th>RESULTS</th>
                        
                        <th class="all">Actions</th>

                    </tr>
                </thead>
                <tbody>
                    <tr class="card" style="cursor: pointer">
                        <th>
                            <label>
                                <input type="checkbox" />
                                <span></span>
                            </label>
                        </th>
                        <td>
                            12/5/2018
                        </td>
                        <td>
                            Dr Aibolit
                        </td>
                        <td>
                            REQUESTED
                        </td>
                        <td>
                            Temp: 98.00 F (36.67 c)
                        </td>
                        
                        <td>
                            <a class='dropdown-trigger btn action-btn option-dropdown-btn' href='javascript:void(0);' data-target='dropdown2'>Options <i class="material-icons right arrow-set">keyboard_arrow_down</i></a>
                            <ul id='dropdown2' class='dropdown-content'>
                                <li><a href="#!" class="option-a"><i class="material-icons">remove_red_eye</i>Preview</a></li>
                                <li><a href="#!" class="option-a"><i class="material-icons">print</i>Print</a></li>
                                <li><a href="#!" class="option-a"><i class="material-icons">share</i>Share</a></li>

                            </ul>

                        </td>

                    </tr>
                </tbody>

            </table>
        </div>
    </div>
</div>
<div class="container">
    <div class="row">
        <div class="col s12">
            <button class="add-btn btn right mb-1" id="vital_btn">Add New Vital</button>
        </div>
    </div>
</div>
<div class="container" id="vital_form" style="display:none">
    <div class="row">
        <form class="form-inline col s12">
            <div class="row">
                <div class="col s12 m6">
                    <div class="input-field pl-0">
                        <p>Date of Exam</p>
                        <input type="text" class="datepicker" placeholder="Select Date">
                    </div>
                </div>
                <div class="col s12 m6">
                    <div class="input-field pl-0">
                        <p>Examiner</p>
                        <input class="text-input" type="text" placeholder="Enter Examiner Name">
                    </div>

                </div>
            </div>
            <div class="row">
                <div class=" col s12 m6">
                    <div class="input-field pl-0">
                        <p>Reason for Exam</p>
                        <input class="text-input" type="text" placeholder="Enter Reason for Exam">
                    </div>
                </div>

            </div>
            <div class="row current">
                <div class="col s6 m2 l3">
                    <h5 class="breadcrumbs-title  color-blue">Exam types</h5>
                </div>
            </div>
            <div class="row">
                <div class="col s12 m6">
                    <p>Blood Pressure</p>
                    <div class="input-field pl-0">
                        <input class="text-input" type="text" placeholder="Enter Blood Pressure">
                    </div>
                </div>
                <div class="col s12 m6">
                    <div class="input-field pl-0">
                        <p>Pulse</p>
                        <input class="text-input" type="text" pattern="Enter Pulse">
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col s12 m6">
                    <p>Respiration</p>
                    <div class="input-field pl-0">
                        <input class="text-input" type="text" placeholder="Enter Respiration">
                    </div>
                </div>
                <div class="col s12 m6">
                    <div class="input-field pl-0">
                        <p>Height</p>
                        <input class="text-input" type="text" pattern="Enter Height">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col s12 m6">
                    <p>Temp Method</p>
                    <div class="input-field pl-0">
                        <input class="text-input" type="text" placeholder="Enter Temp Method">
                    </div>
                </div>
                <div class="col s12 m6">
                    <div class="input-field pl-0">
                        <p>BMI Status</p>
                        <input class="text-input" type="text" pattern="Enter BMI Status">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col s12 m6">
                    <p>BMI</p>
                    <div class="input-field pl-0">
                        <input class="text-input" type="text" placeholder="Enter BMI">
                    </div>
                </div>
                <div class="col s12 m6">
                    <div class="input-field pl-0">
                        <p>Oxygen Saturation</p>
                        <input class="text-input" type="text" pattern="Enter Oxygen Saturation">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col s12 m6">
                    <div class="input-field pl-0">
                        <p>Allergies</p>
                        <input class="text-input" type="text" pattern="Enter Allergies">
                    </div>
                </div>
                <div class=" col s12 m6">
                    <div class="input-field pl-0">
                        <p>Temprature</p>
                        <input class="text-input" type="text" placeholder="Enter Temprature">
                    </div>
                </div>
            </div>
            <div class="row mt-2 mb-2">
                <div class="col s12 m12" style="text-align:center">
                    <a href="javascript:void(0);" class="waves-effect waves-light btn add-btn">Save</a>
                </div>
            </div>

        </form>
    </div>
</div>
