<div class="container" id="test_form" style="display:none">
    <div class="row">
        <form class="form-inline col s12">
            <div class="row">

                <div class="col s12 l6">

                    <div class="input-field  pl-0">
                        <p>
                            Visit/Procedure Category
                        </p>
                        <input class="text-input input-styling " type="text" placeholder="Enter Visit/Procedure Category">
                    </div>
                </div>
                <div class="col s12 m6">

                    <div class="input-field  pl-0">
                        <p>
                            Date/Time
                        </p>
                        <input type="text" class="datepicker" placeholder="Select Date">
                    </div>

                </div>
            </div>
            <div class="row">
                <div class="col s12 m6">

                    <div class="input-field pl-0">
                        <p>
                            Physician
                        </p>
                        <input class="text-input input-styling " type="text" placeholder="Enter Physician Name">
                    </div>

                </div>

                <div class="col s12 m6">

                    <div class="input-field pl-0">
                        <p>
                            ICD Code
                        </p>
                        <input class="text-input input-styling " type="text" placeholder="Enter ICD CODE">

                    </div>
                </div>

            </div>
            <div class="row">
                <div class="col s12 m6">
                    <div class="input-field pl-0">
                        <p>
                            CPT Code
                        </p>
                        <input class="text-input input-styling " type="text" placeholder="Enter CPT CODE">
                    </div>

                </div>
            </div>
            <div class="row mt-2 mb-2">
                <div class="col s12 m12" style="text-align:center">
                    <a href="javascript:void(0);" class="btn add-btn">Save</a>

                </div>
            </div>



        </form>

    </div>
</div>
