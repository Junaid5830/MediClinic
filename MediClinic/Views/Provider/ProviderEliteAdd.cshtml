    var ProviderInfoId = _sessionManager.GetProviderInfoId();
    var Signature = ViewBag.WriteSignature;
    var Action = ViewBag.action;

    var ProviderProfilePic = ViewBag.ProviderImage;
    var ProviderSignature = ViewBag.ProviderSignatureImage;
    var SImage = ViewBag.ProviderSignatureImage;
    var image = ViewBag.ProviderImage;
    var RoleId = _sessionManager.GetRoleId();
    var documentName = "";
    var checkClick = ViewBag.clickValue;
    var LinkURL = "";
    if (checkClick == "Editicon")
    {
        LinkURL = "/Provider/ProviderSummary/" + ViewBag.ProId;

    }
    else
    {
        LinkURL = "/Provider/ProviderEliteList";

    }
    if (Action == "Update")
    {
        documentName = ViewBag.DocumentName;
        if (documentName == "")
        {
            documentName = string.Empty;
        }
        else
        {
            documentName = "/images/" + ViewBag.DocumentName;
        }
    }

    if (image == "")
    {
        image = string.Empty;
    }
    else if (!(image is null))

    {
        image = "/images/" + ViewBag.ProviderImage;
    }
    else
    {

    }
    if (SImage == "")
    {
        SImage = string.Empty;
    }
    else if (!(SImage is null))
