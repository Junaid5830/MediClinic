    margin-bottom: -10px;
}
.smallCap p
{
    margin:0 !important;
    color:white;font-size:12px;
}
.set_Number
{
    background-color:#167f64;
    border-radius:25px;
    padding:5px;
}
.set_Name
{
    border:2px solid #177e64 ;
    margin-bottom:10px;
    border-radius:16px;
    padding:5px;
}
.set_Name p
{
    font-size:12px;
    font-weight:400;
    color:#177e64;
    margin:0 !important;
    text-align:center;

}
@@media (max-width:601px)
{
  .set_ONSmallScreen
  {
      padding-right:1rem !important;
  }
}
.set_a
{
font-size :24px; 
color:black; 
cursor:pointer;
}
hr{
    margin:0.5rem 0 !important
}
    </style>    


}
