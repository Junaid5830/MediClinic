<div id="Emailmodal" class="modal email-model">
    <div class="modal-content">
        <form asp-action="EmailSend" asp-controller="PatientSide" method="post" id="EmailModalForm" enctype="multipart/form-data">

            <div class="row">
                <div class="modal-header">
                    <a id="closemodel" class="close right color-blue modal-close material-icons" data-dismiss="modal">close</a>
                    <h5 class="modal-title color-blue"> Compose New Email</h5>
                </div>

                <div class="input-field col s12">
                    <input id="emailInputModal" type="email" class="form-control" asp-for="EmailBasicDto.ToEmails" placeholder="To:" disabled>
                    <span class="required removeValidation" asp-validation-for="EmailBasicDto.ToEmails"></span>

                </div>
                <div class="input-field col s12">
                    <input id="emailModalSubject" type="text" class="form-control" asp-for="EmailBasicDto.Subject" placeholder="Subject:" />
                    <span class="required removeValidation" asp-validation-for="EmailBasicDto.Subject"></span>


                </div>
                <div class="input-field col s12">
                    <textarea id="emailModalBody" class="materialize-textarea" rows="6" placeholder="Message:" asp-for="EmailBasicDto.Body"></textarea>
                    <span class="required removeValidation" asp-validation-for="EmailBasicDto.Body"></span>
                </div>
                <div class="file-field input-field">
                    <div class="btn">
                        <span>File</span>
                        <input type="file" id="attachment_file" asp-for="EmailBasicDto.attachmentFilename">
                    </div>
                    <div class="file-path-wrapper">
                        <input class="file-path form-control validate" type="text" placeholder="Upload one or more files">
                    </div>
                </div>
                <div class="input-field">
                    <button id="emailModalbtn" class="right btn btn-primary">Send</button>
                    <a class="right btn btn-primary modal-close  mr-2">Cancel</a>
                </div>
            </div>

        </form>
    </div>
