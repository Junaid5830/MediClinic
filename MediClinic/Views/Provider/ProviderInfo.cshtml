    ViewData["Title"] = "Provider Info";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{
    var Signature = ViewBag.WriteSignature;
    var Action = ViewBag.action;
    var ProviderProfilePic = ViewBag.ProviderImage;
    var ProviderSignature = ViewBag.ProviderSignatureImage;
    var SImage = ViewBag.ProviderSignatureImage;
    var image = ViewBag.ProviderImage;

    if (image == "")
    {
        image = string.Empty;
    }
    else if (!(image is null))

    {
        image = "/images/" + ViewBag.ProviderImage;
    }
    else
    {

    }
    if (SImage == "")
    {
        SImage = string.Empty;
    }
    else if (!(SImage is null))
