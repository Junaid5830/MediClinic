                                    isSundayRepeatedSlots = true;
                                }
                            }

                            else if ($(this).attr('name') == 'EndTime' && $(this).val() == "") {
                                $(this).closest('div').find('span').addClass('isError');
                                $(this).closest('div').find('span').removeClass('d-none');
                                isValid = false;
                            }
                            else if ($(this).attr('name') == 'EndTime' && $(this).val() != "") {
                                var status = checkIsDuplicateEntry(null, $(this).val(), 'SundayDiv');
                                if (status) {
                                    isSundayRepeatedSlots = true;
                                }
                            }
                            else if ($(this).attr('name') == 'DepartmentName' && ($(this).val() == "" || $(this).val() == null)) {
                                $(this).closest('div').find('.d-none').addClass('isError');
                                $(this).closest('div').find('.d-none').removeClass('d-none');
