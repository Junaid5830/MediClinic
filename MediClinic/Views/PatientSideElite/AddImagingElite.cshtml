

@{
    ViewData["Title"] = "AddImagingElite";
    Layout = "~/Views/Shared/_LayoutElite.cshtml";
}


@section styles{

   
}
@section scripts{
    <script>
        $(function () {

            $(".select2").select2();
            $('.selectpicker').selectpicker();
        });


    </script>
   
}
<div class="page-wrapper">
    <div class="container">
        <div id="breadcrumbs-wrapper">
            <!-- Search for small screen-->
         
                <div class="row">
                    <div class=" col" margin-top-8">
                        <h5 class="breadcrumbs-title color-blue"> Imaging</h5>
                    </div>

                    <div class="col">
                        <a class="waves-effect waves-light float-right" style="margin-top:-11px" href="/patientSideElite/PatientHippaReleaseImagingElite">
                            <i class="mdi mdi-arrow-left right arrow-icon"></i>

                        </a>
                    </div>

                </div>
     
        </div>
     
           
                <div class="row">
                    <form class="col-sm-12  ">
                        <div class="row">
                            <div class=" col-md-6">
                                <p>Date of Imaging</p>

                                <div class="input-field">
                                    <input type="date" class="text-input" placeholder="select date">
                                </div>
                            </div>
                            <div class=" col-md-6">
                                <p>Time of Imaging</p>
                                <div class="input-field">
                                    <input type="time" class="text-input" placeholder="select time">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6 ">
                                <p>
                                   Reported By

                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select  Reported By</option>

                                        <option value="AK">Bronx NY</option>
                                        <option value="HI">New Jersy</option>


                                    </select>
                                </div>

                            </div>

                            <div class="col-md-6 ">
                                <p>
                                   Type of Image

                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select  Type of Image</option>

                                        <option value="AK">Smart Guy, Esq</option>
                                        <option value="HI">Rosy</option>


                                    </select>
                                </div>

                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-6 ">
                                <p>
                                   Report Status
                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select Report Status</option>
                                        <option value="AK">Yes</option>
                                        <option value="AK">No</option>



                                    </select>
                                </div>

                            </div>

                            <div class="col-md-6 ">
                                <p>
                                    Image Film
                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select Image Film</option>

                                        <option value="AK">N/A</option>



                                    </select>
                                </div>

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6 ">
                                <p>
                                    Diagnosis
                                </p>

                                <div class="input-field">
                                    <select class="select2 form-control  text-input" style="width: 100%;">
                                        <option>Select Diagnosis</option>

                                        <option value="AK">No show</option>



                                    </select>
                                </div>

                            </div>
                        </div>
                        <div class="row mt-2 mb-2">
                            <div class="col s12">
                                <div class="text-right">
                                    <a href="javascript:void(0);" class="waves-effect waves-light btn add-btn">Save</a>

                                </div>
                            </div>
                        </div>
                    </form>
                </div>
     
    </div>
</div>





