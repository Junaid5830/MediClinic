                    else if (sessions.WeekDayName == "Friday") {
                        $('#Friday').prop('checked', true);
                    }
                    else if (sessions.WeekDayName == "Saturday") {
                        $('#Saturday').prop('checked', true);
                    }
                    else if (sessions.WeekDayName == "Sunday") {
                        $('#Sunday').prop('checked', true);
                    }
                }
        }


    </script>
}
