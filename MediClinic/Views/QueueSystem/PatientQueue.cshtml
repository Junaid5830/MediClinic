
@{
    ViewData["Title"] = "PatientQueue";
    Layout = "~/Views/Shared/_PatientLayout.cshtml";
}
@section styles
{
<style>.card .card-content {
    padding: 8px;
    border-radius: 0 0 2px 2px;
    }
hr
{
        border-top: 8px solid #177e64;
    border-radius: 10px;
}
.smallCap
{
    border: 2px solid #177e64;
    background-color: #167f64;
    border-radius: 25px;
    /* padding: 5px; */
    width: 73px;
    text-align: center;
    height: 20px;
    /* margin-top: 3px; */
    margin:0px auto ;
    margin-bottom: -10px;
}
.smallCap p
{
    color:white;font-size:12px;
}
.set_Number
{
    background-color:#167f64;
    border-radius:25px;
    padding:5px;
}
.set_Name
{
    border:2px solid #177e64 ;
    margin-bottom:10px;
    border-radius:16px;
    padding:5px;
}
.set_Name p
{
    font-size:12px;
    font-weight:400;
    color:#177e64;

}
@@media (max-width:601px)
{
  .set_ONSmallScreen
  {
      padding-right:1rem !important;
  }
}
.set_a
{
font-size :24px; 
color:black; 
cursor:pointer;
}
    </style>    


}
