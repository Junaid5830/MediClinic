@model MediClinic.Models.DTOs.DMESuppliesDto.DMESupplyReturnTemplateDto

@{
    Layout = null;
    var empty = "N/A";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SupplyReturnRecipt</title>
    <link rel="stylesheet" type="text/css" href="~/app-assets/css/custom/custom.css">

    <!-- END: VENDOR CSS-->
    <!-- BEGIN: Page Level CSS-->
    <link rel="stylesheet" type="text/css" href="~/app-assets/css/themes/vertical-dark-menu-template/materialize.css">
    <link rel="stylesheet" type="text/css" href="~/app-assets/css/themes/vertical-dark-menu-template/style.css">
    <style>
        .patient-image {
            width: 200px;
            height: 200px;
            margin: 0 75px;
        }

        p.text-dark {
            font-weight: 600 !important;
        }

        hr {
            overflow: hidden;
            height: 1px;
            background-color: #e0e0e0;
            width: 100%;
        }

        .card {
            padding: 15px !important;
        }

        .card-content {
            padding-left: 0px !important;
            padding-right: 0px !important;
        }

        .btn-success:hover {
            color: rgb(255, 255, 255);
            background-color: rgb(33, 136, 56);
            border-color: rgb(30, 126, 52);
        }

        .btn-success {
            margin-right: 20px;
            background-color: rgb(33, 136, 56);
            border-color: rgb(30, 126, 52);
        }

        .btn:focus, .btn-large:focus, .btn-small:focus, .btn-floating:focus {
            background-color: rgb(33, 136, 56) !important;
            border-color: rgb(30, 126, 52) !important;
        }

        [type='checkbox']:checked + span:not(.lever):before {
            margin-top: 14px !important;
        }

        .set_Checkbox p {
            margin-top: 12px !important;
            margin-bottom: 12px !important;
        }

        [type='checkbox'] + span:not(.lever):before {
            margin-top: 14px !important;
        }

        .margin-top-8 {
            margin-top: 15px !important;
        }

        [type='radio']:disabled:not(:checked) + span:before, [type='radio']:disabled:checked + span:before {
            margin-top: 12px;
        }

        [type='radio']:disabled:not(:checked) + span:before, [type='radio']:disabled:checked + span:after {
            margin-top: 12px;
        }
    </style>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            debugger;
            var today = new Date();
            var dd = String(today.getDate()).padStart(2, '0');
            var mm = String(today.getMonth() + 1).padStart(2, '0'); //January is 0!
            var yyyy = today.getFullYear();
            today = mm + '/' + dd + '/' + yyyy;
            $('#dateArea').html(today);

             var itemNames = @Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(ViewBag.Items));
                for (var i = 0; i < itemNames.length; i++) {

                  $('#prescribedItemsDiv').append("<div class='col s6 m6'><div class='input-field'> <p> "+itemNames[i]+" :  <span><b>Duration: N/A</b> </span></p></div></div>")

                }
        })

        function printdiv() {
            $("#arrowIcon").hide();
            $(".btn-success").hide();
            window.print();
            $("#arrowIcon").show();
            $(".btn-success").show();
        }
    </script>

</head>

<body>
    <div class="content-area content-right">
        <div class="app-wrapper">




            <div class="container">
                <div class="row">
                    <div class="col s12 m9 margin-top-8">
                        <h5 class="breadcrumbs-title color-blue card-title">
                            SUPPLY RETURN RECEIPT/AKNOWLEDGMENT
                        </h5>
                    </div>


                    <div class="col s3 m3 margin-top-8">
                        <a id="arrowIcon" class="waves-effect waves-light float-right mr-2 mt-2" href="DmeSupplies">
                            <i class="material-icons right arrow-icon">arrow_back</i>

                        </a>
                    </div>
                </div>
            </div>
            <div class="row margin-top-8">
                <div class="col  m12  ">


                    <div class="row">
                        <div class="col m6 s6">
                            <div class="input-field">
                                <h6>
                                    Rental Start Date:<span>
                                        <b>
                                            @empty
                                        </b>
                                    </span>
                                </h6>
                            </div>
                        </div>
                        <div class="col m6 s6">
                            <div class="input-field">
                                <h6>
                                    Delivery Date:<span>
                                        <b>
                                            @empty
                                        </b>
                                    </span>
                                </h6>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col m6 s6">
                            <div class="input-field">
                                <h6>
                                    Treatment End Date:<span>
                                        <b>
                                            @empty
                                        </b>
                                    </span>
                                </h6>
                            </div>
                        </div>
                        <div class="col m6 s6">
                            <div class="input-field">
                                <h6>
                                    Pick Up/completion Date:<span>
                                        <b>
                                            @empty
                                        </b>
                                    </span>
                                </h6>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col  m12  ">
                            <h5 class=" color-blue ">PRESCRIBED SUPPLIES/EQUIPMENT</h5>
                            <div class="row" id="prescribedItemsDiv">
                              
                            </div>

                        </div>
                    </div>
                    <div class="row">
                        <div class="col m12 s12">
                            <p>
                                I, <b>@Model.PatientName</b> HAVE RETURNED THE ABOVE MENTIONED SUPPLY(IES) ON
                                /______/______IN THE SAME CONDITION AS WAS DELIVERED TO ME ON  <b>@empty</b>
                            </p>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col m6 s6">
                            <div class="input-field">
                                <h6>
                                    Patient Or Authorized Representative Name:<span>
                                        <b>
                                            @Model.PatientName
                                        </b>
                                    </span>
                                </h6>
                            </div>
                        </div>
                        <div class="col m6 s6">


                            <div class="input-field">
                                <h6>Patient Or Authorized Representative Signature:<img src="~/SignatureImages/@Model.PatientSignature" style="width:300px" class="ml-1" alt="patient signature" /></h6>
                            </div>

                        </div>
                    </div>
                    <div class="row">
                        <div class="col m6 s6">
                            <div class="input-field">
                                <h6>
                                    Date Of Execution/delivery:<span>
                                        <b>
                                            @empty
                                        </b>
                                    </span>
                                </h6>
                            </div>
                        </div>
                        <div class="col m6 s6">


                            <div class="input-field">
                                <h6>
                                    Relationship To Patient/reason For Signing:<span>
                                        <b>
                                            @empty
                                        </b>
                                    </span>
                                </h6>
                            </div>

                        </div>
                    </div>
                    <div class="row">
                        <div class="col m6 s6">
                            <div class="input-field">
                                <h6>Absolute Medical & Surgical Supplies, Inc. Rep. Signature: @empty</h6>
                            </div>
                        </div>
                        <div class="col m6 s6">


                            <div class="input-field">
                                <h6>
                                    Date:<span>
                                        <b id="dateArea">
                                        </b>
                                    </span>
                                </h6>
                            </div>

                        </div>
                    </div>
                    <div class="row mb-2">
                        <div class="col-md-12">
                            <button class="btn btn-success float-right" onclick="printdiv();">Print</button>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</body>
</html>
