<script>
    $('.select2').select2();
  
    $(".for-hide").change(function () {
   
        var optionsText = this.options[this.selectedIndex].text;
      
        if (this.value != 0) {
            
         
            $(this).parent().siblings().removeClass("hidden");
            if (optionsText == "Date Of Visit") {
                $(this).parent().siblings('div').children('input').attr("placeholder", "Pic Date");
                $(this).parent().siblings('div').children('input').attr("type", "Date");
            } else {
                $(this).parent().siblings('div').children('input').attr("type", "text");
                $(this).parent().siblings('div').children('input').attr("placeholder", optionsText);
            }
            
            
           
        }
        if(this.value==0)
        {
            $(this).parent().siblings().addClass("hidden");
        }

        
    });
</script>

