<body>
        <div class="container">
            <div class="date-time">@DateTime.Now</div>
            <div class="row">
                <div class="col-sm-12 col-lg-12 col-xl-12">
                    <!--Gradient Card-->
                    <div id="cards-extended">

                        <div class="row">
                            <div class="col-12 col-xs-12 col-sm-12 col-md-5 col-lg-6">
                                <h5 class="color-blue">Provider</h5>
                            </div>

                            <div class="col-12 col-xs-12 col-sm-12 col-md-5 col-lg-6 right  set_icons">

                                <a class="waves-effect waves-light right no-print" href="@Url.Action("ProviderSummary","provider", new {Id = ProviderInfoId })">
                                    <i class="mdi mdi-arrow-left right arrow-icon"></i>
                                </a>
                                <a class="pr-1 no-print" onclick="window.print()"  style="cursor:pointer">
                                    <i class="mdi mdi-printer"></i>
                                </a>

                            </div>


                        </div>

                        <div class="row">
                            <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">
                                @if (Model.ProviderSummary.ProviderProfilePic != null)
                                {

                                    <img src="~/images/@Model.ProviderSummary.ProviderProfilePic" class=" patient-image circle responsive-img grey lighten-1 padding-4" />
                                }
                                else
                                {
                                    <img src="~/app-assets/images/user/male-user.png" class=" patient-image circle responsive-img grey lighten-1 padding-4" />
                                }
                            </div>
                            <div class="col-xs-12 col-md-10 col-lg-10">
                                <div class="row">
                                    @if (Model.ProviderSummarySettingDto.ProviderIdPrint)
                                    {
                                        


                                        <div class="col-6 col-xs-6 col-sm-6 col-md-3 col-lg-3">
                                            <p class=" mb-0">ID</p>
                                            <p class=" mt-0 font-weight-600 black-text">@ViewBag.ProviderID</p>

                                        </div>
                                    }
                                    @if (Model.ProviderSummarySettingDto.Name)
                                    {
                                        var fullame = (Model.ProviderSummary.FullName != null) ? Model.ProviderSummary.FullName : "N/A";


                                        <div class="col-6 col-xs-6 col-sm-6 col-md-3 col-lg-3">
                                            <p class=" mb-0">Name</p>
                                            <p class=" mt-0 font-weight-600 black-text">@fullame</p>

                                        </div>
                                    }
                                    @if (Model.ProviderSummarySettingDto.Speciality)
                                    {
                                        var speciality = (Model.ProviderSummary.Speciality != null) ? Model.ProviderSummary.Speciality : "N/A";
                                        <div class="col-6 col-xs-6 col-sm-6 col-md-3 col-lg-3">
                                            <p class="mb-0">Speciality</p>
                                            <p class=" mt-0 font-weight-600 black-text">@speciality</p>
                                        </div>
                                    }
                                    @if (Model.ProviderSummarySettingDto.Title)
                                    {
                                        var title = (Model.ProviderSummary.Title != null) ? Model.ProviderSummary.Title : "N/A";
                                        <div class="col-6 col-xs-6 col-sm-6 col-md-3 col-lg-3">
                                            <p class="mb-0">Title</p>
                                            <p class="mt-0 black-text font-weight-600">@title</p>
                                        </div>
                                    }
                                    @if (Model.ProviderSummarySettingDto.Suffix)
                                    {
                                        var suffix = (Model.ProviderSummary.Suffix != null) ? Model.ProviderSummary.Suffix : "N/A";
                                        <div class="col-6 col-xs-6 col-sm-6 col-md-3 col-lg-3">
                                            <p class="mb-0">Suffix</p>
                                            <p class=" mt-0 font-weight-600 black-text">@suffix</p>
                                        </div>
                                    }

                                    @if (Model.ProviderSummarySettingDto.LicenseNo)
                                    {
                                        var licence = (Model.ProviderSummary.LicenseNo != null) ? Model.ProviderSummary.LicenseNo : "N/A";
                                        <div class="col-6 col-xs-6 col-sm-6 col-md-3 col-lg-3">
                                            <p class=" mb-0">License No</p>
                                            <p class=" mt-0 font-weight-600 black-text">@licence</p>
                                        </div>
                                    }
                                    @if (Model.ProviderSummarySettingDto.NpiNumber)
                                    {
                                        var npi = (Model.ProviderSummary.NpiNumber != null) ? Model.ProviderSummary.NpiNumber : "N/A";
                                        <div class="col-6 col-xs-6 col-sm-6 col-md-3 col-lg-3">
                                            <p class="mb-0">NPI No</p>
                                            <p class=" mt-0 font-weight-600 black-text">@npi</p>
                                        </div>
                                    }
                                    @if (Model.ProviderSummarySettingDto.PhoneNo)
                                    {
                                        var phn = (Model.ProviderSummary.PhoneNo != null) ? Model.ProviderSummary.PhoneNo : "N/A";
                                        <div class="col-6 col-xs-6 col-sm-6 col-md-3 col-lg-3">
                                            <p class="mb-0">Phone Number</p>
                                            <p class="mt-0 black-text font-weight-600">@phn</p>

                                        </div>
                                    }
                                    @if (Model.ProviderSummarySettingDto.Email)
                                    {
                                        var mail = (Model.ProviderSummary.Email != null) ? Model.ProviderSummary.Email : "N/A";
                                        <div hidden class="col-6 col-xs-6 col-sm-6 col-md-3 col-lg-3">
                                            <p class="mb-0">Email</p>
                                            <p class="mt-0 black-text font-weight-600">@mail</p>

                                        </div>
                                    }
                                    @if (Model.ProviderSummarySettingDto.EntityName)
                                    {
                                        var entity = (Model.ProviderSummary.EntityName != null) ? Model.ProviderSummary.EntityName : "N/A";
                                        <div class="col-6 col-xs-6 col-sm-6 col-md-3 col-lg-3">
                                            <p class="mb-0">PC/EntityName</p>
                                            <p class=" mt-0 font-weight-600 black-text">@entity</p>
                                        </div>
                                    }

                                    @if (Model.ProviderSummarySettingDto.Address)
                                    {
                                        var address = (Model.ProviderSummary.AddressLine1 != null) ? Model.ProviderSummary.AddressLine1 : "N/A";
                                        <div class="col-6 col-xs-6 col-sm-6 col-md-3 col-lg-3">
                                            <p class=" mb-0">Address</p>
                                            <p class=" mt-0 font-weight-600 black-text">@address</p>
                                        </div>
                                    }

                                    @if (Model.ProviderSummarySettingDto.DateCreated)
                                    {
                                        var dC = (Model.ProviderSummary.CreatedDate != null) ? Model.ProviderSummary.CreatedDate.ToString("MM/dd/yyyy") : "N/A";
                                        <div class="col-6 col-xs-6 col-sm-6 col-md-3 col-lg-3">
                                            <p class=" mb-0">Date Created</p>
                                            <p class=" mt-0 font-weight-600 black-text">@dC</p>
                                        </div>
                                    }


                                </div>
                            </div>
                        </div>

                        <div class="divider mt-2"></div>
                    </div>
                    <!--Gradient Card-->
                    <div id="cards-extended">
                        @if (Model.ProviderSummarySettingDto.ScheduleListing)
                        {
                            <div class="row">
                                <div class="col-12 col-xs-12 col-sm-12 col-md-5 col-lg-6">
                                    <h5 class="color-blue">Schedule</h5>
                                </div>

                                


                            </div>

                            <div class="row ">
                                <div class="col-lg-12">
                                    <div class="table-responsive">
                                        <table class="table table-striped table-bordered table-sm">
                                            <thead>
                                                <tr class="uppercase">
                                                    <th>Date</th>
                                                    <th>Name of day</th>
                                                    <th>Start Time</th>
                                                    <th>End Time</th>
                                                    <th>Department</th>


                                                </tr>
                                            </thead>
                                            <tbody>
                                                @if (!(Model.ScheduleList is null))
                                                {
                                                    @foreach (var item in Model.ScheduleList)
                                                    {
                                                        <tr>
                                                            <td>@item.WeekDate.Value.ToString(SystemFormat.DateFormat())</td>

                                                            <td>@item.WeekDayName</td>
                                                            <td>@item.STime</td>
                                                            <td>@item.ETime</td>
                                                            <td>@item.DepartmentName</td>


                                                        </tr>
                                                    }
                                                }


                                            </tbody>

                                        </table>
                                    </div>
                                </div>
                            </div>
                        }

                    </div>
                </div>
            </div>
        </div>
    <script src="~/Elite/_node_modules/jquery/jquery-3.2.1.min.js"></script>
  
</body>


