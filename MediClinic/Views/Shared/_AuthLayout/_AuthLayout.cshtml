    <script>
        function selectPackage(PackageName) {
            $("#packageName").val(PackageName);
            $("#selectPackage").attr("disabled", false);

        }
        function PayPal() {
            var package = $("#packageName").val();
            debugger;
            if (package == 'Basic') {
                $("#paypal-button-container").show();
                $("#paypal-button-container2").hide();

            }
            else {
                $("#paypal-button-container2").show();
                $("#paypal-button-container").hide();
            }
        }
        function Credit() {
            $("#paypal-button-container").hide();
            $("#paypal-button-container2").hide();

        }
       
        $(document).ready(function () {
            $("#selectPackage").attr("disabled", true);
            $("#Login-btn").on("click", function () {
                $("#main2").show();
                $("#customer-from").hide();
                $("#newaccount").hide();
                $("#Login-btn").hide();
                $("#oldacc").hide();
            });
            $("#SignUp-Link").on("click", function () {

                $("#customer-from").show();
                $("#oldacc").hide();
                $("#newaccount").hide();
                $("#oldacc1").show();
                $("#Login-btn").hide();


            });
            $("#SignIn-Link").on("click", function () {
                debugger;
                $("#customer-from").hide();
                $("#main2").show();
                $("#newaccount2").show();
            });
            $("#SignUp-Link2").on("click", function () {

                $("#customer-from").show();
                $("#oldacc").hide();
                $("#newaccount").hide();
                $("#oldacc1").show();
                $("#Login-btn").hide();
                $("#main2").hide();

            });
            var current_fs, next_fs, previous_fs; //fieldsets
            var opacity;

            $(".next").click(function () {

                current_fs = $(this).parent();
                next_fs = $(this).parent().next();

                //Add Class Active
                $("#progressbar li").eq($("fieldset").index(next_fs)).addClass("active");

                //show the next fieldset
                next_fs.show();
                //hide the current fieldset with style
                current_fs.animate({ opacity: 0 }, {
                    step: function (now) {
                        // for making fielset appear animation
                        opacity = 1 - now;

                        current_fs.css({
                            'display': 'none',
                            'position': 'relative'
                        });
                        next_fs.css({ 'opacity': opacity });
                    },
                    duration: 600
                });
            });

            $(".previous").click(function () {

                current_fs = $(this).parent();
                previous_fs = $(this).parent().prev();

                //Remove class active
                $("#progressbar li").eq($("fieldset").index(current_fs)).removeClass("active");

                //show the previous fieldset
                previous_fs.show();

                //hide the current fieldset with style
                current_fs.animate({ opacity: 0 }, {
                    step: function (now) {
                        // for making fielset appear animation
                        opacity = 1 - now;

                        current_fs.css({
                            'display': 'none',
                            'position': 'relative'
                        });
                        previous_fs.css({ 'opacity': opacity });
                    },
                    duration: 600
                });
            });

            $('.radio-group .radio').click(function () {
                $(this).parent().find('.radio').removeClass('selected');
                $(this).addClass('selected');
            });

            $(".submit").click(function () {
                return false;
            })

        });
        function AuthClientSubmit(data) {
            //if (data.success == "SuccessFully Created") {
            //    window.location.reload();
            //}
            if (data.success) {

                $('input').val('');
                debugger;
                $.confirm({
                    title: 'Confirm!',
                    content: 'Thank you for sign up. We have sent you email for further detail.',
                    buttons: {
                        confirm: function () {
                            window.location.reload();
                        }

                    }
                });
            }
            else {
                new PNotify({
                    title: 'Error!',
                    text: 'Can not Save, Already In Drop Down',
                    delay: 5000,
                    type: 'error',
                    addclass: 'pnotify-center'
                });

            }
        }
        function AuthFormSubmit(data) {
            debugger;
            if (data.isError) {
                $(".required").html(data.errorMessage);
            }
            else {
                var connection = new signalR.HubConnectionBuilder()
                    .withUrl("/NotificationHub")
                    .configureLogging(signalR.LogLevel.Information)
                    .build();
                connection.start().then(() => {
                    debugger;
                    $.post("/Auth/NotificationInfo", { connectionId: connection.connectionId }).done(function (data) {
                    });
                });
                $(".required").html("");
                window.location.href = data;

            }
        }
    </script>
