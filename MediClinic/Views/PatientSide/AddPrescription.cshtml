                                <a href="@Url.Action("Prescription", "PatientSide",new {Id = patientInfoId })" class="waves-effect waves-light Recreate btn">Cancel</a>
                            </div>
                        </div>
                    </form>
                </div>



            </div>
            <div id="test2" class="col s12">
