                        <div class="input-field">
                            <input type="date" class="text-input" placeholder="select date">
                        </div>

                    </div>
                    <div class="col-md-6 ">
                        <p>
                            Items or Items Group Name/No


                        </p>

                        <div class="input-field">
                            <select class="select2 form-control  text-input" style="width: 100%;">
                                <option>Select</option>

                                <option value="AK">EMS Unit</option>
                                <option value="AK">Shoulder Support</option>
                                <option value="AK">Orthopedic Car Seat</option>



                            </select>
                        </div>

                    </div>

                </div>
                <div class="row">

                    <div class=" col-md-6 ">
                        <p>CPT Code</p>

                        <div class="input-field">
                            <select class="select2 form-control  text-input" style="width: 100%;">
                                <option>Select</option>

                                <option value="AK">100</option>
                                <option value="AK">1002</option>
                                <option value="AK">2001</option>



                            </select>
                        </div>
                    </div>
                    <div class="col-md-6 ">
                        <p>
                            ICD Code

                        </p>

                        <div class="input-field">
                            <select class="select2 form-control  text-input" style="width: 100%;">
                                <option>Select</option>

                                <option value="AK">100</option>
                                <option value="AK">1002</option>
                                <option value="AK">2001</option>



                            </select>
                        </div>

                    </div>

                </div>
                <div class="row">
                    <div class=" col-md-6 ">
                        <p>Prescriber</p>

                        <div class="input-field">
