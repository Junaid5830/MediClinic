            <div class="col s6 m3 l3">
                <p>REFERRAL NAME</p>
                <div class="switch">
                    <input type="checkbox" id="REFERRALCheckBox" onchange="checkBoxChangeListSettings(this)" data-val="Pregnent" asp-for="PatientListSettingDto.REFERRALNAME" data-toggle="toggle" />
                </div>
            </div>

        </div>

    </div>
</div>