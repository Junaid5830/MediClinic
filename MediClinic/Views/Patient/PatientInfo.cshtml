                                        <ul id='dropdown10' class='dropdown-content dropdown-content-custom'>
                                            <li><a href="~/Home/_AddPatientPJ" class="text-sub"><span style="font-size:15px;">P.I (Personal Injury)</span></a></li>
                                            <li><a href="~/Home/_AddPatientCollection" class="text-sub"><span style="font-size:15px;">Collection/Arbitration Attorney</span></a></li>
                                        </ul>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <a href="javscript:void(0);" data-target="email-sidenav" class="sidenav-trigger hide-on-large-only btn Recreate btn-floating"><i class="material-icons">menu</i></a>
                    </div>
                </div>
            </div>
            <!-- Sidebar Area Ends -->
            <!-- Content Area Starts -->
            <div class="app-email" id="pat-info">
                <div class="row">


                    <div class="col s12 right">
                        <a class="waves-effect waves-light float-right mt-2" href="/patient/patientlist">
                            <i class="material-icons  right arrow-icon">arrow_back</i>

                        </a>
                    </div>
                </div>
                <partial name="~/Views/_PatientPartialView/_PatientInfo.cshtml">

            </div>


        </div>
    </div>
</div>
