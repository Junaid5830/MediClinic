
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.css">

    <title>PrintTables</title>
    <style>
        .fa {
            font-size: 35px;
        }

        .card {
            padding: 22px 15px;
            margin: 10px 0;
            box-shadow: 1px 1px 12px 1px #e2e2e2 !important;
        }

            .card p {
                font-weight: 600;
                font-size: 13px;
                color: #03a9f4;
                margin-bottom: 0 !important;
            }

            .card img {
                width: 30px;
                height: 30px;
                margin: 7px 20px 0 20px;
            }

        .FLName {
            width: 150px;
            padding: 55px !important;
            background-color: #f1f1f1 !important;
            border-radius: 50% !important;
            height: 150px !important;
            margin: auto !important;
        }

        p.mb-0 {
            color: #6b6f82;
        }

        p.text-dark {
            font-weight: 600 !important;
        }

        .patient-image {
            width: 200px;
            height: 200px;
            margin: 0 75px;
        }

        hr {
            overflow: hidden;
            height: 1px;
            background-color: #e0e0e0;
            width: 100%;
        }

        ul {
            list-style: none;
        }

            ul img {
                width: 50px;
                height: 50px;
                border-radius: 50%;
            }

        .relatedcard {
            margin: 5px 11px !important;
        }

        .FLNameLetter {
            width: 40px;
            height: 40px;
            background-color: #f1f1f1;
            border-radius: 50%;
            padding: 8px 10px;
        }

        .warning {
            padding: 10px !important;
            border-radius: 4px;
        }
        .checkClassForUnPrint{display:none!important; }
    </style>
</head>
<body>
   
    <div class="container mt-5 mb-5" id="main-div-Print">
        <div class="row">
            <div class="col-md-6">
                <h2 class="text-primary">Data</h2>
            </div>
            <div class="col-md-6">
                <a class="waves-effect waves-light float-right" style="margin-top: -1px;cursor:pointer;" onclick="goBack()">
                    <i class="fa fa-arrow-left"></i>

                </a>
            </div>
            <div class="row">
                @ViewBag.Data
            </div>
            <a class="btn btn-success waves-effect text-left no-print " id="print">Custom Print</a>
        </div>
    </div>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js"></script>
    <script src="https://printjs-4de6.kxcdn.com/print.min.js"></script>
    <script src="~/lib/jquery.unobtrusive-ajax.min.js"></script>
    <script>
        function goBack() {
            window.history.back();
        }
        $("#print").click(function () {
            window.print();
        });
       
    </script>
</body>
</html>
